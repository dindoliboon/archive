#include <windows.h>

100 DIALOG 6, 18, 246, 215
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
STYLE WS_MINIMIZEBOX | WS_MAXIMIZEBOX | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU | WS_THICKFRAME
CAPTION "My Password Viewer"
FONT 8, "MS Shell Dlg"
BEGIN
