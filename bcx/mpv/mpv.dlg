    LTEXT           "&Website:", 105, 2, 118, 50, 8, SS_NOTIFY
    EDITTEXT        106, 55, 118, 189, 12, ES_AUTOHSCROLL
    LTEXT           "&User Name:", 107, 2, 133, 50, 8, SS_NOTIFY
    EDITTEXT        108, 55, 133, 189, 12, ES_AUTOHSCROLL
    LTEXT           "&Password:", 109, 2, 148, 50, 8, SS_NOTIFY
    EDITTEXT        110, 55, 148, 189, 12, ES_PASSWORD | ES_AUTOHSCROLL
    LTEXT           "&Comments:", 111, 2, 163, 50, 8, SS_NOTIFY
    EDITTEXT        112, 55, 163, 189, 50, ES_MULTILINE | ES_AUTOVSCROLL | ES_AUTOHSCROLL | WS_VSCROLL | WS_HSCROLL | ES_WANTRETURN
